package sec1;

import java.net.InetAddress;
import java.net.UnknownHostException;

public class NetworkExam3 {
	public static void main(String[] args) {
		//본인 컴퓨터의 IP주소 와 컴퓨터 이름 출력
			try {
				InetAddress local = InetAddress.getLocalHost();
				System.out.println("본인 컴퓨터 ip 주소 : "+local.getHostAddress());
				System.out.println("네트워크상 컴퓨터 이름 : "+local.getHostName());
				InetAddress[] iArr = InetAddress.getAllByName("www.google.com");
				System.out.println("www.google.com");
				for(InetAddress addr : iArr) {
					System.out.println(addr.getHostAddress());
				}
			} catch (UnknownHostException e) {
				e.printStackTrace();
			}
		
		}
		//www.google.com의 연결 IP 주소를 출력
	}
